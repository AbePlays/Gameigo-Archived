seState } from \"react\";\nimport { useHistory, useLocation, useParams } from \"react-router-dom\";\nimport parse from \"html-react-parser\";\nimport { useQuery } from \"react-query\";\nimport { useDispatch, useSelector } from \"react-redux\";\n\nimport Spinner from \"../Spinner\";\nimport { UserInfoState } from \"../../store/reducers/types\";\nimport { Game } from \"../Home\";\nimport { AddData } from \"../../store/actions/AddData\";\nimport { RemoveData } from \"../../store/actions/RemoveData\";\n\ninterface ParamTypes {\n  id: string;\n}\n\ninterface LocationType {\n  game: Game;\n}\n\ninterface ImageType {\n  id: number;\n  image: string;\n}\n\ninterface GameInfo {\n  name: string;\n  description: string;\n  released: string;\n  background_image: string;\n  website: string;\n  metacritic: number;\n  playtime: number;\n  parent_platforms: {\n    platform: {\n      id: number;\n      name: string;\n    };\n  }[];\n  genres: {\n    id: number;\n    name: string;\n  }[];\n  stores: {\n    id: number;\n    url: string;\n    store: {\n      id: number;\n      name: string;\n    };\n  }[];\n}\n\nexport default function GameDetails(): ReactElement {\n  const { id } = useParams<ParamTypes>();\n  const { state } = useLocation<LocationType>();\n  const history = useHistory();\n  const [gameIsFavorite, setGameIsFavorite] = useState(false);\n\n  let details: GameInfo | null = null;\n\n  const disptach = useDispatch();\n  const userId = useSelector(\n    (state: { userInfo: UserInfoState }) => state.userInfo.uid\n  );\n  const favorites: Game[] = useSelector(\n    (state: { userInfo: UserInfoState }) => state.userInfo.favorites\n  );\n\n  useEffect(() => {\n    console.log(\"[GAMEDETAILS] UEF\");\n    for (const fav of favorites) {\n      if (fav.id === state.game.id) {\n        setGameIsFavorite(true);\n        break;\n      }\n    }\n  }, []);\n\n  const getData = async () => {\n    const res = await fetch(`https://api.rawg.io/api/games/${id}`);\n    return res.json();\n  };\n\n  const formatDate = (releasedDate: string): string => {\n    const date = new Date(releasedDate).toDateString();\n    let arr = date.split(\" \");\n    return arr[1] + \" \" + arr[2] + \", \" + arr[3];\n  };\n\n  const { isLoading, error, data } = useQuery(id, getData);\n\n  if (data) {\n    details = {\n      name: data.name,\n      description: data.description,\n      released: data.released,\n      background_image: data.background_image,\n      website: data.website,\n      metacritic: data.metacritic,\n      playtime: data.playtime,\n      parent_platforms: data.parent_platforms,\n      genres: data.genres,\n      stores: data.stores,\n    };\n  }\n\n  return (\n    <div className=\"dark:bg-black min-h-screen bg-gray-50\">\n      {isLoading && <Spinner />}\n      {error && (\n        <p className=\"text-white text-center py-8\">Error while fetching data</p>\n      )}\n      {details && (\n        <div className=\"w-screen relative bg-black\">\n          <div className=\"absolute top-0 left-0 right-0 z-100\">\n            <img\n              className=\"w-full h-full\"\n              src={details.background_image}\n              alt=\"lol\"\n            />\n          </div>\n          <div\n            className=\"absolute top-0 left-0 z-11  w-full h-full\"\n            style={{\n              opacity: 0.7,\n              backgroundColor: \"black\",\n            }}\n          />\n          <div className=\"max-w-screen-lg mx-auto py-6 px-4 z-10 relative text-white\">\n            <svg\n              className=\"w-8 cursor-pointer\"\n              xmlns=\"http://www.w3.org/2000/svg\"\n              fill=\"none\"\n              viewBox=\"0 0 24 24\"\n              stroke=\"currentColor\"\n              onClick={() => {\n                history.goBack();\n              }}\n            >\n              <path\n                strokeLinecap=\"round\"\n                strokeLinejoin=\"round\"\n                strokeWidth={2}\n                d=\"M10 19l-7-7m0 0l